  t2 = p0_x-pxb_x;
  t3 = p0_y-pxb_y;
  t4 = p0_z-pxb_z;
  t5 = p0_x-pxf_x;
  t6 = p0_y-pxf_y;
  t7 = p0_z-pxf_z;
  t9 = t2*t2;
  t10 = t3*t3;
  t11 = t4*t4;
  t12 = t9+t10+t11;
  t13 = 1.0/sqrt(t12);
  t14 = t5*t5;
  t15 = t6*t6;
  t16 = t7*t7;
  t17 = t14+t15+t16;
  t18 = 1.0/sqrt(t17);
  t8 = t2*t13+t5*t18;
  t19 = t3*t13+t6*t18;
  t20 = t4*t13+t7*t18;
  t0 = (t8*t8)*(1.0/4.0)+(t19*t19)*(1.0/4.0)+(t20*t20)*(1.0/4.0);
