  t2 = len_ex_b*p0_x+len_ex_f*p0_x-len_ex_f*pxb_x-len_ex_b*pxf_x;
  t3 = 1.0/(len_ex_b*len_ex_b);
  t4 = 1.0/(len_ex_f*len_ex_f);
  t5 = len_ex_b*p0_y+len_ex_f*p0_y-len_ex_f*pxb_y-len_ex_b*pxf_y;
  t6 = len_ex_b*p0_z+len_ex_f*p0_z-len_ex_f*pxb_z-len_ex_b*pxf_z;
  t0 = (t2*t2)*t3*t4*4.0+t3*t4*(t5*t5)*4.0+t3*t4*(t6*t6)*4.0;
