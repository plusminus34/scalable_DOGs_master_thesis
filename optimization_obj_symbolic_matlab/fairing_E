  t3 = 1.0/len_ex_b;
  t4 = 1.0/len_ex_f;
  t5 = 1.0/len_ex_ff;
  t2 = t3*(p0_x-pxb_x)*2.0+t4*(p0_x-pxf_x)*4.0-t5*(pxf_x-pxff_x)*2.0;
  t6 = t3*(p0_y-pxb_y)*2.0+t4*(p0_y-pxf_y)*4.0-t5*(pxf_y-pxff_y)*2.0;
  t7 = t3*(p0_z-pxb_z)*2.0+t4*(p0_z-pxf_z)*4.0-t5*(pxf_z-pxff_z)*2.0;
  t0 = t2*t2+t6*t6+t7*t7;
