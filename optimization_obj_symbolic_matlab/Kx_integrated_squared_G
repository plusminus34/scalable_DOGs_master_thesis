  t2 = pxb_x*2.0;
  t3 = pxf_x*2.0;
  t4 = p0_x*-4.0+t2+t3;
  t5 = pxb_y*2.0;
  t6 = pxf_y*2.0;
  t7 = p0_y*-4.0+t5+t6;
  t8 = pxb_z*2.0;
  t9 = pxf_z*2.0;
  t10 = p0_z*-4.0+t8+t9;
  A0[0][0] = p0_x*8.0-pxb_x*4.0-pxf_x*4.0;
  A0[1][0] = p0_y*8.0-pxb_y*4.0-pxf_y*4.0;
  A0[2][0] = p0_z*8.0-pxb_z*4.0-pxf_z*4.0;
  A0[3][0] = t4;
  A0[4][0] = t7;
  A0[5][0] = t10;
  A0[6][0] = t4;
  A0[7][0] = t7;
  A0[8][0] = t10;
