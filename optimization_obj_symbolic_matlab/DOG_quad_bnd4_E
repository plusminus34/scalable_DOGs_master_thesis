  t2 = p0_x-pxb_x;
  t3 = p0_y-pxb_y;
  t4 = p0_z-pxb_z;
  t6 = p0_x-pyf_x;
  t7 = t2*t6;
  t8 = p0_y-pyf_y;
  t9 = t3*t8;
  t10 = p0_z-pyf_z;
  t11 = t4*t10;
  t5 = -t7-t9-t11+t2*(p0_x-pyb_x)+t3*(p0_y-pyb_y)+t4*(p0_z-pyb_z);
  t12 = t7+t9+t11-t6*(p0_x-pxf_x)-t8*(p0_y-pxf_y)-t10*(p0_z-pxf_z);
  t0 = t5*t5+t12*t12;
