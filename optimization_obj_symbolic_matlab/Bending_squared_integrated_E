  t3 = 1.0/len_ex_b;
  t4 = 1.0/len_ex_f;
  t5 = 1.0/len_ey_b;
  t6 = 1.0/len_ey_f;
  t2 = t3*(p0_x-pxb_x)+t4*(p0_x-pxf_x)+t5*(p0_x-pyb_x)+t6*(p0_x-pyf_x);
  t7 = t3*(p0_y-pxb_y)+t4*(p0_y-pxf_y)+t5*(p0_y-pyb_y)+t6*(p0_y-pyf_y);
  t8 = t3*(p0_z-pxb_z)+t4*(p0_z-pxf_z)+t5*(p0_z-pyb_z)+t6*(p0_z-pyf_z);
  t0 = t2*t2+t7*t7+t8*t8;
