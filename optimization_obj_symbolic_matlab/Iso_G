  t2 = p0_x-pxf_x;
  t3 = p0_y-pxf_y;
  t4 = p0_z-pxf_z;
  t5 = t2*t2;
  t6 = t3*t3;
  t7 = t4*t4;
  t8 = -l0+t5+t6+t7;
  t9 = p0_x*2.0;
  t10 = pxf_x*2.0;
  t11 = t9-t10;
  t12 = t8*t11*2.0;
  t13 = p0_y*2.0;
  t14 = pxf_y*2.0;
  t15 = t13-t14;
  t16 = t8*t15*2.0;
  t17 = p0_z*2.0;
  t18 = pxf_z*2.0;
  t19 = t17-t18;
  t20 = t8*t19*2.0;
  A0[0][0] = l0*2.0-t5*2.0-t6*2.0-t7*2.0;
  A0[1][0] = t12;
  A0[2][0] = t16;
  A0[3][0] = t20;
  A0[4][0] = -t12;
  A0[5][0] = -t16;
  A0[6][0] = -t20;
