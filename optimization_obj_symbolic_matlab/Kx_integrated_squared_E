  t2 = p0_x-pxb_x;
  t3 = p0_y-pxb_y;
  t4 = p0_z-pxb_z;
  t5 = p0_x-pxf_x;
  t6 = p0_y-pxf_y;
  t7 = p0_z-pxf_z;
  t9 = t2*t2;
  t10 = t3*t3;
  t11 = t4*t4;
  t12 = t9+t10+t11;
  t13 = t5*t5;
  t14 = t6*t6;
  t15 = t7*t7;
  t16 = t13+t14+t15;
  t8 = sqrt(t12)+sqrt(t16);
  t18 = 1.0/sqrt(t12);
  t19 = 1.0/sqrt(t16);
  t17 = t2*t18+t5*t19;
  t20 = t3*t18+t6*t19;
  t21 = t4*t18+t7*t19;
  t0 = (t8*t8)*(t17*t17+t20*t20+t21*t21)*(1.0/1.6E1);
