  t2 = p0_x-pxb_x;
  t3 = p0_y-pxb_y;
  t4 = p0_z-pxb_z;
  t5 = p0_x-pxf_x;
  t6 = p0_y-pxf_y;
  t7 = p0_z-pxf_z;
  t8 = t2*t2;
  t9 = t3*t3;
  t10 = t4*t4;
  t11 = t8+t9+t10;
  t12 = t5*t5;
  t13 = t6*t6;
  t14 = t7*t7;
  t15 = t12+t13+t14;
  t17 = 1.0/sqrt(t11);
  t18 = 1.0/sqrt(t15);
  t16 = t2*t17+t5*t18;
  t19 = t3*t17+t6*t18;
  t20 = t4*t17+t7*t18;
  t0 = (sqrt(t11)*(1.0/2.0)+sqrt(t15)*(1.0/2.0))*sqrt(t16*t16+t19*t19+t20*t20)*(1.0/2.0);
